<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="btn_next" parent="btn_text">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">50dp</item>
        <item name="layout_constraintWidth_percent">0.89</item>
        <item name="android:background">@drawable/btn_before</item>
        <item name="android:layout_marginBottom">30dp</item>
    </style>
    <style name="btn_text">
        <item name="android:fontFamily">@font/himchan_medium</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="edit">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="layout_constraintWidth_percent">0.89</item>
        <item name="android:singleLine">false</item>
        <item name="android:fontFamily">@font/himchan_light</item>
        <item name="hintTextColor">#6E6E87</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">18sp</item>
        <item name="android:background">@drawable/edit_background</item>
    </style>

    <style name="lay_login_edit">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="layout_constraintWidth_percent">0.89</item>
        <item name="hintEnabled">false</item>
    </style>
    <style name="edit_login" parent="edit">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">54dp</item>
    </style>

    <style name="text_item_tag">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_marginEnd">5dp</item>
        <item name="android:textColor">@color/main</item>
        <item name="android:fontFamily">@font/himchan_light</item>
    </style>
    <style name="tab_text" parent="@android:style/TextAppearance.Widget.TabWidget">
        <item name="android:fontFamily">@font/himchan_medium</item>
        <item name="android:fontStyle">normal</item>
        <item name="android:textSize">21sp</item>
    </style>

    <style name="text_club_tag">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/himchan_medium</item>
        <item name="android:textColor">@color/main</item>
        <item name="android:textSize">23sp</item>
        <item name="android:layout_marginEnd">12dp</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>

    <style name="text_club_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/himchan_medium</item>
        <item name="android:textSize">21sp</item>
        <item name="android:layout_marginTop">16dp</item>
    </style>
    <style name="text_club_content">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/inter_light</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginTop">5dp</item>
    </style>

    <style name="card_club_toggle">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">5dp</item>
        <item name="strokeWidth">0dp</item>
        <item name="android:layout_marginStart">3dp</item>
    </style>
    <style name="img_club_toggle">
        <item name="android:layout_width">10dp</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/drop_down</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>
    <style name="text_club_toggle_grade">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/himchan_medium</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_marginStart">16dp</item>
    </style>
    <style name="text_club_toggle_member">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:fontFamily">@font/himchan_light</item>
        <item name="android:textSize">14sp</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_marginTop">3dp</item>
    </style>
</resources>